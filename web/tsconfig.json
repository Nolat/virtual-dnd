{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext", "esnext.asynciterable"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/common/components/*"],
      "components": ["src/common/components"],
      "definitions/*": ["src/common/definitions/*"],
      "definitions": ["src/common/definitions"],
      "modules/*": ["src/modules/*"],
      "modules": ["src/modules"],
      "pages/*": ["src/pages/*"],
      "pages": ["src/pages"],
      "modules/*": ["src/modules/*"],
      "modules": ["src/modules"],
      "public/*": ["public/*"],
      "public": ["public"],
      "test/*": ["test/*"],
      "test": ["test"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx","test/index.jsx"],
  "exclude": ["node_modules", "test/*", "cypress/*"]
}
